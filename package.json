{
  "name": "react-albus",
  "version": "2.0.0",
  "description": "React component library for building declarative multi-step flows.",
  "files": [
    "lib"
  ],
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "start": "webpack-dev-server --port 3000 --inline --hot --open",
    "test": "jest",
    "lint": "eslint --ignore-path .eslintignore --ext .js --ext .jsx ./",
    "prebuild": "yarn run clean && yarn run lint",
    "build": "babel src -d lib --copy-files",
    "posttest": "yarn run lint",
    "prepublish": "yarn run build"
  },
  "repository": "americanexpress/react-albus",
  "keywords": [
    "react",
    "react-component",
    "wizard",
    "step",
    "multistep",
    "routing"
  ],
  "authors": [
    "Jack Cross <jack.cross1@aexp.com> (https://github.com/jayjaycross)",
    "Nathan Force <nathan.a.force@aexp.com> (https://github.com/nathanforce)"
  ],
  "license": "Apache-2.0",
  "jest": {
    "preset": "amex-jest-preset-react",
    "testPathIgnorePatterns": [
      ".eslintrc.js"
    ]
  },
  "dependencies": {
    "history": "^4.6.0",
    "hoist-non-react-statics": "^2.3.1",
    "prop-types": "^15.5.8"
  },
  "devDependencies": {
    "amex-jest-preset-react": "^5.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.6",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.5.0",
    "eslint-config-amex": "^11.1.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "import-glob-loader": "^1.1.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-router-dom": "^5.1.1",
    "react-test-renderer": "^16.10.1",
    "react-transition-group": "^4.3.0",
    "rc-progress": "^2.5.2",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  }
}
